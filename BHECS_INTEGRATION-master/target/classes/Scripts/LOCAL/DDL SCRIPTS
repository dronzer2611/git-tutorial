    costing_request_detail_id integer,
    company_code character varying(15) COLLATE pg_catalog."default",
    cost_center character varying(15) COLLATE pg_catalog."default",
    project_code character varying(15) COLLATE pg_catalog."default",
    product_line character varying(15) COLLATE pg_catalog."default",
    reference_code character varying(15) COLLATE pg_catalog."default",
    legacy_adn character varying(100) COLLATE pg_catalog."default",
    created_by character varying(15) COLLATE pg_catalog."default",
    created_date timestamp without time zone,
    last_updated_by character varying(15) COLLATE pg_catalog."default",
    last_updated_date timestamp without time zone,
    function_code character varying(15) COLLATE pg_catalog."default"
)
